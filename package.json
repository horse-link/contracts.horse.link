{
  "name": "solidity",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "hh:compile": "npx hardhat compile",
    "hh:deploy": "npx hardhat deploy --network hardhat --tags underlying,oracle,registryToken,vault,market",
    "deploy:production": "yarn deploy:prod_arbitrum && yarn deploy:prod_goerli",
    "deploy:dev": "yarn deploy:dev_arbitrum && yarn deploy:dev_goerli",
    "deploy:prod_arbitrum": "npx hardhat clean && npx hardhat compile && npx hardhat deploy --network prod_arbitrum --tags oracle,vault,market,registry --export ./deployments/prod_arbitrum/prod_arbitrum.json",
    "deploy:prod_arbitrum:registry": "npx hardhat clean && npx hardhat compile && npx hardhat deploy --network prod_arbitrum --tags registry",
    "deploy:dev_arbitrum": "npx hardhat clean && npx hardhat compile && npx hardhat deploy --network dev_arbitrum  --tags oracle,vault,market,registry --export ./deployments/dev_arbitrum/dev_arbitrum.json",
    "deploy:prod_goerli": "npx hardhat clean && npx hardhat compile && npx hardhat deploy --network prod_goerli --tags oracle,vault,market --export ./deployments/prod_goerli/prod_goerli.json",
    "deploy:dev_goerli": "npx hardhat clean && npx hardhat compile && npx hardhat deploy --network dev_goerli --tags oracle,vault,market --export ./deployments/dev_goerli/dev_goerli.json",
    "deploy:dev_goerli:registry": "npx hardhat clean && npx hardhat compile && npx hardhat deploy --network dev_goerli --tags registry",
    "verify:prod_goerli": "npx hardhat --network prod_goerli etherscan-verify --solc-input",
    "verify:dev_goerli": "npx hardhat --network dev_goerli etherscan-verify --solc-input",
    "verify:prod_arbitrum": "npx hardhat --network prod_arbitrum etherscan-verify --solc-input",
    "verify:dev_arbitrum": "npx hardhat --network dev_arbitrum etherscan-verify --solc-input",
    "deploy:local": "npx hardhat compile && npx hardhat deploy --network localhost --tags underlying,oracle,registryToken,vault,market --export ./deployments/localhost/localhost.json",
    "hh:node": "npx hardhat node --hostname localhost",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage",
    "format": "prettier --write \"scripts/**/*.{js,ts}\" && prettier --write \"contracts/**/*.sol\"",
    "lint": "eslint --ext .ts --fix scripts && yarn format",
    "lint-staged": "lint-staged",
    "scratch": "ts-node scripts/scratch.ts",
    "settle:prod_goerli": "ts-node scripts/settle.ts prod_goerli",
    "settle:dev_goerli": "ts-node scripts/settle.ts dev_goerli",
    "settle:prod_arbitrum": "ts-node scripts/settle.ts prod_arbitrum",
    "settle:dev_arbitrum": "ts-node scripts/settle.ts dev_arbitrum",
    "force": "ts-node scripts/force_settle.ts prod_arbitrum",
    "analyze": "ts-node scripts/analytics.ts prod_arbitrum"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "@ethersproject/providers": "^5.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@tenderly/hardhat-tenderly": "^1.6.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.13.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy-tenderly": "^0.2.0",
    "hardhat-upgrades": "^0.0.0",
    "keccak": "^3.0.3",
    "papaparse": "^5.3.2",
    "rlp": "^2.2.6",
    "ts-node": "^10.0.0",
    "typechain": "^6.1.0",
    "typescript": "^4.3.4"
  },
  "resolutions": {
    "@ledgerhq/hw-app-eth": "6.7.0",
    "@ledgerhq/hw-transport": "6.7.0",
    "ethers": "^5.4.7"
  },
  "devDependencies": {
    "@ethersproject/hardware-wallets": "^5.4.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.8.3",
    "@openzeppelin/contracts-upgradeable": "^4.8.3",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@remix-project/remixd": "^0.6.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "erc721a": "^3.2.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hardhat-deploy": "^0.11.28",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "pretty-quick": "^3.1.1",
    "solidity-coverage": "^0.8.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
